{
  "name": "alexantr/yii2-elfinder",
  "description": "elFinder file manager for Yii 2",
  "keywords": ["yii2", "widget", "elfinder", "file manager"],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Yashkin",
      "email": "alex@yashkin.by"
    }
  ],
  "support": {
    "issues": "https://github.com/alexantr/yii2-elfinder/issues",
    "source": "https://github.com/alexantr/yii2-elfinder"
  },
  "require": {
    "yiisoft/yii2-jui": "~2.0.0",
    "studio-42/elfinder": "^2.1.49"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^7.5"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "alexantr\\elfinder\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.7.x-dev"
    }
  },
  "config": {
    "fxp-asset": {
      "enabled": false
    }
  }
}
